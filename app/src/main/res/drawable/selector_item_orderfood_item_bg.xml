<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/radius_12px" />
            <stroke android:width="@dimen/line_space_height_s" android:color="@color/main_color1" />
            <gradient android:centerColor="@color/color3" android:endColor="@color/white" android:gradientRadius="@dimen/radius_12px" android:startColor="@color/white" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/radius_12px" />
            <stroke android:width="@dimen/line_space_height_s" android:color="@color/main_color1" />
            <gradient android:centerColor="@color/color2" android:endColor="@color/color3" android:gradientRadius="@dimen/radius_12px" android:startColor="@color/color3" />
        </shape>
    </item>
</selector>